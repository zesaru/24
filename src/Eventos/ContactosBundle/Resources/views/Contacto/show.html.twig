            <tr>
                <th>Fechaderespuesta</th>
                <td>{{ entity.fechaderespuesta|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Via</th>
                <td>{{ entity.via }}</td>
            </tr>
            <tr>
                <th>Contesto</th>
                <td>{{ entity.contesto }}</td>
            </tr>
            <tr>
                <th>Ingresohora</th>
                <td>{{ entity.ingresohora|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Ingreso</th>
                <td>{{ entity.ingreso }}</td>
            </tr>
            <tr>
                <th>Hora</th>
                <td>{{ entity.hora }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ entity.codigo }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('contacto') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contacto_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
