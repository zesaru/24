            <tr>
                <th>Fax</th>
                <td>{{ entity.fax }}</td>
            </tr>
            <tr>
                <th>Movil</th>
                <td>{{ entity.movil }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ entity.observaciones }}</td>
            </tr>
            <tr>
                <th>Fechadeactualizacion</th>
                <td>{{ entity.fechadeactualizacion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Impresion</th>
                <td>{{ entity.impresion }}</td>
            </tr>
            <tr>
                <th>Horadeimpresion</th>
                <td>{{ entity.horadeimpresion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Enviadas</th>
                <td>{{ entity.enviadas }}</td>
            </tr>
            <tr>
                <th>Horadeenvio</th>
                <td>{{ entity.horadeenvio|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Numinvitados</th>
                <td>{{ entity.numInvitados }}</td>
