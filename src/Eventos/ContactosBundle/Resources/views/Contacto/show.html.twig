{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Contacto</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Vocativo</th>
                <td>{{ entity.vocativo }}</td>
            </tr>
            <tr>
                <th>Nombres</th>
                <td>{{ entity.nombres }}</td>
            </tr>
            <tr>
                <th>Apellidos</th>
                <td>{{ entity.apellidos }}</td>
            </tr>
            <tr>
                <th>Organizacion</th>
                <td>{{ entity.organizacion }}</td>
            </tr>
            <tr>
                <th>Titulo1</th>
                <td>{{ entity.titulo1 }}</td>
            </tr>
            <tr>
                <th>Titulo2</th>
                <td>{{ entity.titulo2 }}</td>
            </tr>
            <tr>
                <th>Dir1</th>
                <td>{{ entity.dir1 }}</td>
            </tr>
            <tr>
                <th>Dir2</th>
                <td>{{ entity.dir2 }}</td>
            </tr>
            <tr>
                <th>Codigopostal</th>
